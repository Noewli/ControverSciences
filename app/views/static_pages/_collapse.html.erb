<div class="panel panel-default">
    <div class="panel-heading" role="tab" id="heading<%= id %>">
        <h3 class="panel-title clearfix">
          <a data-toggle="collapse" href="#Collapse<%= id %>" class="pull-right glyphicon glyphicon-chevron-down"></a>
            <a data-toggle="collapse" href="#Collapse<%= id %>" aria-expanded="true" aria-controls="Collapse<%= id %>">
               <%= title %>
            </a>
        </h3>
    </div>
    <div id="Collapse<%= id %>" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
        <div class="panel-body">
          <p>
           <%= content %>
          </p>
        </div>
    </div>
</div>
